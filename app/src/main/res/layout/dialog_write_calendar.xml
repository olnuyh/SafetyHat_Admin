<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WriteCalendarActivity">

    <FrameLayout
        android:layout_width="327dp"
        android:layout_height="440dp"
        android:background="@drawable/login_corner"
        android:layout_marginTop="115dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="20dp" >

    <TextView
        android:id="@+id/calendarStartTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="160dp"
        android:text="시작 날짜"
        android:textSize="13sp"
        android:fontFamily="@font/a13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/calendarStart"
        android:layout_width="225dp"
        android:layout_height="30dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="185dp"
        android:background="@drawable/stroke_round"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageButton
        android:id="@+id/calendarStartBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="260dp"
        android:layout_marginTop="180dp"
        android:background="@android:color/transparent"
        android:src="@drawable/calendar_icon"
        android:text="시작달력 아이콘 이미지"
        app:layout_constraintStart_toEndOf="@+id/calendarStart"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/calendarEndTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="235dp"
        android:fontFamily="@font/a13"
        android:text="종료 날짜"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/calendarEnd"
        android:layout_width="225dp"
        android:layout_height="30dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/stroke_round"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/calendarEndBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="260dp"
        android:layout_marginTop="255dp"
        android:background="@android:color/transparent"
        android:src="@drawable/calendar_icon"
        android:text="마감 달력 아이콘 이미지"
        app:layout_constraintStart_toEndOf="@+id/calendarEnd"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/calendarRegisterTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="60dp"
        android:text="일정 내용"
        android:fontFamily="@font/a13"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/calendarContents"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/stroke_round"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/calendarRegisterBtn"
        android:layout_width="138dp"
        android:layout_height="40dp"
        android:layout_marginTop="150dp"
        android:background="@android:color/transparent"
        android:src="@drawable/calendar_ok"
        android:layout_gravity="center"
        android:text="등록"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
